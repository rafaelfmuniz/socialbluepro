# Configurações do npm para SocialBluePro
# Garante que as versões sejam instaladas corretamente

# Não usar cache para garantir versões frescas
cache=/tmp/npm-cache
prefer-offline=false

# Forçar resolução de peer dependencies
legacy-peer-deps=true

# Log detalhado
loglevel=warn

# Timeouts
fetch-timeout=60000
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
